{
	"version": 1,
	"base_image": "codeocean/jupyterlab:3.6.1-miniconda4.12.0-python3.9-ubuntu20.04",
	"post_install": true,
	"options": {
		"registry_host_arg": true,
		"git_ask_pass": true
	},
	"installers": {
		"apt": {
			"packages": [
				{
					"name": "tree",
					"version": "1.8.0-1"
				}
			],
			"install_software_properties_common": true
		},
		"vscode": {
			"packages": [
				{
					"name": "REditorSupport.R"
				},
				{
					"name": "continue.continue"
				},
				{
					"name": "ms-python.python"
				},
				{
					"name": "ms-toolsai.jupyter"
				},
				{
					"name": "reageyao.bioSyntax"
				},
				{
					"name": "saoudrizwan.claude-dev"
				}
			],
			"version": "4.95.3"
		}
	}
}